<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/admin/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/blog/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/visitante/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/admin/index.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>AdministrarBlogs</from-outcome>
   <to-view-id>/admin/AdministrarBlogs.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>AdministrarUsuariosBlogger</from-outcome>
   <to-view-id>/admin/AdministrarUsuariosBlogger.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/blog/index.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>Articulos</from-outcome>
   <to-view-id>/blog/Articulos.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/visitante/index.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>ArticulosVisitante</from-outcome>
   <to-view-id>/visitante/ArticulosVisitante.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
